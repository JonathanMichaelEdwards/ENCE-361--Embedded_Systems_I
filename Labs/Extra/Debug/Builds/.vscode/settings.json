{
    "files.associations": {
        "hw_memmap.h": "c",
        "sysctl.h": "c",
        "gpio.h": "c",
        "hi.h": "c",
        "hello.h": "c",
        "cstdint": "cpp",
        "random": "cpp",
        "*.tcc": "cpp",
        "delay.h": "c",
        "delay_2.h": "c"
    },

    "actionButtons": {
        "defaultColor": "light-grey",
        "reloadButton": " ",
        "commands": [
            {
                "name": "Build $(checklist)",
                "singleInstance": true,
                "command": "cd Debug/Builds && make build",
            }, {
                "name": "Flash  $(arrow-right)",
                "singleInstance": true,
                "command": "cd Debug/Builds && make flash",
            }, {
                "name": "Build - Flash $(checklist) $(arrow-right)",
                "singleInstance": true,
                "command": "cd Debug/Builds && make all",
            }
        ]
    },
}